item, index) in data.items\" :key=\"index\">\n            <a :href=\"item.link\" target=\"_blank\" rel=\"noopener noreferrer\">\n              <img :src=\"item.image\" :title=\"item.text\" />\n            </a>\n          </div>\n        </a-carousel>\n      </div>\n      <!-- Promo state 3 -->\n      <div v-else id=\"promo_3\">\n        <div class=\"promo_title\">{{ data.title || 'Sponsor' }}</div>\n        <a-button type=\"primary\" ghost @click=\"popupInfo\">{{\n          data.btnText || 'Become a Sponsor'\n        }}</a-button>\n      </div>\n    </div>\n    <a-divider v-if=\"data\" dashed id=\"reset-margin\" />\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Promo',\n  methods: {\n    gotoLink() {\n      window.open(this.data.link);\n    },\n    popupInfo() {\n      this.$info({\n        title: this.data.msgTitle || 'Message Title',\n        content: this.data.msgText || 'Put your text here.',\n        okText: this.data.msgOkText || 'Ok',\n        maskClosable: true\n      });\n    }\n  },\n  computed: {\n    data() {\n      return this.$themeConfig.ads;\n    }\n  }\n};\n</script>\n\n<style lang=\"less\">\n@import '../styles/palette.less';\n\n.promo {\n  padding: 0 1rem 0.6rem;\n  font-size: 0.75rem;\n  overflow: hidden;\n\n  #promo_1 {\n    cursor: pointer;\n\n    img {\n      float: left;\n      margin-right: 1rem;\n      max-width: @PromoStyle1MW;\n    }\n  }\n\n  #promo_2 {\n    img {\n      width: 100%;\n    }\n  }\n\n  #promo_3 {\n    padding: 0 1.5rem;\n    margin-bottom: 0.8125rem;\n\n    .promo_title {\n      color: #777;\n      font-weight: 300;\n      font-size: 0.9375rem;\n      margin-bottom: 0.9375rem;\n    }\n  }\n}\n\n#reset-margin {\n  margin: 5px 0;\n}\n\n@media (max-width: @MQMobile) {\n  .promo {\n    display: @MobileShow;\n  }\n  #reset-margin {\n    display: @MobileShow;\n  }\n}\n</style>\n"]}]}